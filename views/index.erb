<html>
  <head>
    <title>xix</title>
  </head>
  <body style="
      background-color: rgb(210,231,241);
      background-image: url('dhaffner.png');
      background-position: 50% 100%;
      background-repeat: no-repeat;
  ">
  <img src='dduussttiinn.jpg' id='bunny' style="display:none; margin:auto;">
    <script type="text/javascript">
    self.setInterval(xix, 3000);
    function xix()
    {
      var dust = document.getElementById('bunny')
      if(dust.style.display == 'block')
        dust.style.display = 'none';
      else
        dust.style.display = 'block';
    }
    </script>
  </div>
  </body>
</html>